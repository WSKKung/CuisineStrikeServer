{
	"files": [
		"./index.ts"
	],
	"include": [
		"./src/communications/*",
		"./src/*"
	],
	"compilerOptions": {
	  "target": "es5",
	  "outFile": "./build/index.js",
	  "typeRoots": [
		"./node_modules"
	  ],
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	}
  }