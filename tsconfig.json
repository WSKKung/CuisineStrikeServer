{
	"files": [
		"./index.ts"
	],
	"include": [
		"./src/*",
		"./src/cards/*",
		"./src/test/*",
		"./src/communications/*"
	],
	"compilerOptions": {
		"target": "es5",
		"outFile": "./build/index.js",
		"typeRoots": [
			"./node_modules"
		],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
  }